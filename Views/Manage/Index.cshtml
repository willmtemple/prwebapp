@Html.Partial("_StatusMessage", Model.StatusMessage)
<div class="ui divided section list">
    <label class="ui horizontal label">Email</label>
    @Model.Email
</div>

@section Scripts {
    @await Html.PartialAsync("_ValidationScriptsPartial")
}
